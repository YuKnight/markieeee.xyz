<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
	
		
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>id</key>
			<string>HAPTIC_FEEDBACK</string>
			<key>footerText</key>
			<string>Haptic feedback produces a small vibration on successful authentication.</string>
			
		</dict>
		
		<dict>
			<key>PostNotification</key>
			<string>net.limneos.bioprotect.prefsChanged</string>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>net.limneos.bioprotect</string>
			<key>key</key>
			<string>hapticSetting</string>
			<key>label</key>
			<string>Enabled</string>
		</dict>
		
		
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>id</key>
			<string>HAPTIC_SETTINGS</string>
			<key>footerText</key>
			<string>You may adjust the intensity of the haptic feedback.</string>
			
		</dict>
		
		
		
		<dict>
			<key>PostNotification</key>
			<string>net.limneos.bioprotect.hapticChanged</string>
			<key>cell</key>
			<string>PSSliderCell</string>
			<key>default</key>
			<integer>2</integer>
			<key>min</key>
			<integer>1</integer>
			<key>max</key>
			<integer>3</integer>
			<key>isSegmented</key>
			<true/>
			<key>segmentCount</key>
			<integer>2</integer>
			<key>defaults</key>
			<string>net.limneos.bioprotect</string>
			<key>key</key>
			<string>haptic</string>
			<key>id</key>
			<string>HAPTIC_FEEDBACK</string>
		</dict>
	
		
	
	
		
	</array>
	<key>title</key>
	<string>Haptic Feedback</string>
</dict>
</plist>
